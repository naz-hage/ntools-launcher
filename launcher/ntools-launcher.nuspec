  <metadata>
    <id>ntools-launcher</id>
    <version>0.0.0.0</version>
    <title>A wrapper for Process .NET class</title>
    <authors>naz-hage</authors>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
